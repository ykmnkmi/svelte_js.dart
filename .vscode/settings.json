{
  "dart.lineLength": 120,
  "dart.sdkPath": ".sdk",
  "editor.rulers": [
    80,
    120
  ],
  "files.associations": {
    "*.svelte": "html"
  },
  "yaml.schemas": {
    "https://json.schemastore.org/dart-build": [
      "build.yaml",
      "*.build.yaml",
      "build.*.yaml"
    ]
  },
  "[html]": {
    "files.trimTrailingWhitespace": false
  }
}